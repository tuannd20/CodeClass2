<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">
<form >
  <div class="form-group">
    <label for="exampleInputPassword1">Full Name</label>
    <input type="text" class="form-control" id="exampleInputPassword1" name="fullName" value="<%= getTrainer.fullName %>" placeholder="" readonly>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Specialty</label>
    <input type="text" class="form-control" id="exampleInputPassword1" name="specially"  value="<%= getTrainer.specially %>" readonly>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Age</label>
    <input type="number" class="form-control" id="exampleInputPassword1" name="age" value="<%= getTrainer.age %>" readonly>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Address</label>
    <input type="text" class="form-control" id="exampleInputPassword1" name="address" value="<%= getTrainer.address %>" readonly>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Email</label>
    <input type="email" class="form-control" id="exampleInputPassword1" name="email" value="<%= getTrainer.email %>" readonly>
  </div>
</form>